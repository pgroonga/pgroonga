require_relative "../pgroonga-package-task"

class PostgreSQL11PGDGPGroongaPackageTask < VersionedPGroongaPackageTask
  def initialize
    super("11", "-pgdg")
  end

  def apt_targets_default
    [
      "ubuntu-bionic",
      "ubuntu-bionic-arm64",
      "ubuntu-focal",
      "ubuntu-focal-arm64",
      "ubuntu-jammy",
      "ubuntu-jammy-arm64",
    ]
  end

  def enable_ubuntu?
    false
  end

  def enable_yum?
    false
  end
end

task = PostgreSQL11PGDGPGroongaPackageTask.new
task.define
