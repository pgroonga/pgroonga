require_relative "../pgroonga-package-task"

class PostgreSQL13PGroongaPackageTask < VersionedPGroongaPackageTask
  def initialize
    super("13")
  end

  def apt_targets_default
    []
  end

  def enable_ubuntu?
    false
  end

  def enable_yum?
    false
  end
end

task = PostgreSQL13PGroongaPackageTask.new
task.define
